{
  "metadata": [
    {
      "src": [
        {
          "files": [
            "src/**/*.csproj"
          ],
          "exclude": [
            "**/obj/**",
            "**/bin/**"
          ]
        }
      ],
      "dest": "api",
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "includePrivateMembers": false,
      "properties": {
        "TargetFramework": "net9.0"
      }
    }
  ],
  "build": {
    "content": [
      {
        "files": [
          "api/**.yml",
          "api/index.md"
        ]
      },
      {
        "files": [
          "articles/**.md",
          "articles/**/toc.yml",
          "toc.yml",
          "*.md"
        ]
      }
    ],
    "resource": [
      {
        "files": [
          "images/**"
        ]
      }
    ],
    "overwrite": [
      {
        "files": [
          "apidoc/**.md"
        ],
        "exclude": [
          "obj/**",
          "_site/**"
        ]
      }
    ],
    "dest": "_site",
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "markdownEngineName": "markdig",
    "noLangKeyword": false,
    "keepFileLink": false,
    "cleanupCacheHistory": false,
    "disableGitFeatures": false,
    "globalMetadata": {
      "_appTitle": "Orleans.GpuBridge.Core",
      "_appName": "Orleans.GpuBridge.Core",
      "_appFooter": "Copyright Â© 2025 Orleans.GpuBridge.Core. GPU-Native Distributed Computing for .NET",
      "_enableSearch": true,
      "_gitContribute": {
        "repo": "https://github.com/mivertowski/Orleans.GpuBridge.Core",
        "branch": "main"
      },
      "_gitUrlPattern": "github"
    }
  }
}
